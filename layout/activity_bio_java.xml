<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BioJava">

    <TextView
        android:id="@+id/bioSeq1"
        android:layout_width="295dp"
        android:layout_height="185dp"
        android:text="AACTGACGGGTGAACGGTCAGTACGGGATTTTACCATGCGATGACGTACGATGAAAAATTGGGTCGTGGCTCCGGCT"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/bioSeq2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/bioSeq2"
        android:layout_width="291dp"
        android:layout_height="179dp"
        android:text="AACTGACGGGTGAACGGTCAGTACGGGATTTTACCATGCGATGACGTACGATGAAAAATGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCGCTGGCTCCGGCT"
